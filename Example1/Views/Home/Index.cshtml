@model List<Example1.Models.Product>

<!DOCTYPE html>
<html>
<head>
    <title>Product List</title>
</head>
<body>
    <h1>Product List</h1>
    <ul>
        @foreach (var product in Model)
        {
            <li>
                <strong>@product.Name</strong> - $@product.Price
            </li>
        }
    </ul>
</body>
</html>
